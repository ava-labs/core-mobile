{
  "expo": {
    "name": "k2-alpine",
    "slug": "k2-alpine",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "platforms": [
      "ios",
      "android"
    ],
    "plugins": [
      "expo-font",
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": false,
          "supportsPictureInPicture": false
        }
      ]
    ],
    "android": {
      "permissions": ["android.permission.DETECT_SCREEN_CAPTURE"]
    },
    "newArchEnabled": true
  }
}
