{
  "expo": {
    "scheme": "core",
    "name": "AvaxWallet",
    "slug": "core-wallet",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "plugins": [
      "./plugins/useWithAndroidNightTheme.js",
      [
        "@vonovak/react-native-theme-control"
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "extraPods": [
              {
                "name": "SDWebImage",
                "modular_headers": true
              },
              {
                "name": "SDWebImageSVGCoder",
                "modular_headers": true
              }
            ]
          }
        }
      ],
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      ["expo-router", { "root": "./app/new/routes" }],
      [
        "expo-camera",
        {
          "recordAudioAndroid": false
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./app/assets",
            "./app/new/assets"
          ]
        }
      ],
      "@config-plugins/detox",
      [
        "react-native-share",
        {
          "ios": [
            "twitter"
          ],
          "android": [
            "com.twitter.android"
          ],
          "enableBase64ShareAndroid": true
        }
      ],
      "expo-localization",
      "expo-video",
      "@react-native-community/datetimepicker",
      "@sentry/react-native"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
