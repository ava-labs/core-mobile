{
  "expo": {
    "scheme": "core",
    "name": "AvaxWallet",
    "displayName": "Core Mobile",
    "slug": "core-wallet",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-router",
      [
        "expo-camera",
        {
          "recordAudioAndroid": false
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./app/assets", "./app/new/assets"
          ]
        }
      ],
      "@config-plugins/detox",
      [
        "react-native-share",
        {
          "ios": [
            "twitter",
            "fb-messenger",
            "tg",
            "whatsapp"
          ],
          "android": [
            "com.twitter.android",
            "com.facebook.orca",
            "org.telegram.messenger",
            "com.whatsapp"
          ],
          "enableBase64ShareAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
