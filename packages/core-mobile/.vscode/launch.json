    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/e2e/tests/plusIcon/send/sendSepoliaEthTo2ndAccount.e2e.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }, 
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Detox",
            "program": "${workspaceFolder}/node_modules/.bin/detox",
            "args": ["test", "e2e/tests/plusIcon/send.e2e.ts", "-c", "ios.internal.debug", "--colors", "--loglevel", "trace"],
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": null,
            "env": {
              "NODE_ENV": "development",
            },
            "console": "integratedTerminal"
          } 
    ]
}