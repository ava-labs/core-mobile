{
  "name": "@avalabs/k2-mobile",
  "main": "src/index.ts",
  "scripts": {
    "setup": "yarn allow-scripts",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "podInstall": "bundle _2.1.4_ install && cd ios && bundle exec pod install",
    "postinstall": "node_modules/.bin/patch-package",
    "test": "jest",
    "lint": "eslint .",
    "tsc": "tsc -p ."
  },
  "dependencies": {
    "dripsy": "4.3.3",
    "react": "18.1.0",
    "react-native": "0.70.6"
  },
  "devDependencies": {
    "@avalabs/eslint-config-mobile": "workspace:*",
    "@avalabs/tsconfig-mobile": "workspace:*",
    "@babel/core": "7.22.11",
    "@babel/runtime": "7.22.11",
    "@lavamoat/allow-scripts": "2.0.3",
    "@rushstack/eslint-patch": "1.5.1",
    "@types/jest": "29.5.6",
    "@types/react": "17.0.44",
    "@types/react-dom": "18.0.6",
    "@types/react-native": "0.66.4",
    "@types/react-test-renderer": "18.0.0",
    "babel-jest": "29.7.0",
    "deprecated-react-native-prop-types": "2.3.0",
    "eslint": "8.50.0",
    "jest": "29.7.0",
    "metro-react-native-babel-preset": "0.72.3",
    "patch-package": "6.4.7",
    "react-test-renderer": "18.2.0",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "react": "18.1.0",
    "react-native": "0.70.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix --max-warnings 0"
  },
  "lavamoat": {
    "allowScripts": {
      "$root$": true
    }
  }
}
