{
  "name": "@avalabs/k2-mobile",
  "main": "src/index.ts",
  "scripts": {
    "setup": "yarn allow-scripts",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "podInstall": "bundle _2.1.4_ install && cd ios && bundle exec pod install",
    "postinstall": "node_modules/.bin/patch-package",
    "test": "jest --passWithNoTests",
    "lint": "eslint .",
    "tsc": "tsc -p .",
    "storybook-generate": "sb-rn-get-stories",
    "storybook-watch": "sb-rn-watcher"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.18.1",
    "@react-native-community/datetimepicker": "7.1.0",
    "@react-native-community/slider": "4.4.2",
    "dripsy": "4.3.3",
    "react": "18.1.0",
    "react-dom": "18.2.0",
    "react-native": "0.70.14",
    "react-native-safe-area-context": "4.5.3"
  },
  "devDependencies": {
    "@avalabs/eslint-config-mobile": "workspace:*",
    "@avalabs/tsconfig-mobile": "workspace:*",
    "@babel/core": "7.22.11",
    "@babel/runtime": "7.22.11",
    "@lavamoat/allow-scripts": "2.0.3",
    "@rushstack/eslint-patch": "1.5.1",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-controls": "6.5.16",
    "@storybook/addon-links": "6.5.12",
    "@storybook/addon-ondevice-actions": "6.5.3",
    "@storybook/addon-ondevice-backgrounds": "6.5.3",
    "@storybook/addon-ondevice-controls": "6.5.3",
    "@storybook/react-native": "6.5.3",
    "@types/jest": "29.5.6",
    "@types/react": "17.0.44",
    "@types/react-dom": "18.0.6",
    "@types/react-native": "0.66.4",
    "@types/react-test-renderer": "18.0.0",
    "@types/tinycolor2": "1.4.5",
    "babel-jest": "29.7.0",
    "deprecated-react-native-prop-types": "2.3.0",
    "eslint": "8.50.0",
    "jest": "29.7.0",
    "metro-react-native-babel-preset": "0.72.3",
    "patch-package": "6.4.7",
    "react-test-renderer": "18.2.0",
    "tinycolor2": "1.6.0",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "react": "18.1.0",
    "react-native": "0.70.14"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix --max-warnings 0"
  },
  "lavamoat": {
    "allowScripts": {
      "$root$": true,
      "@storybook/addon-links>core-js": false,
      "@storybook/react-native>@storybook/core-client>webpack>watchpack>watchpack-chokidar2>chokidar>fsevents": false
    }
  }
}
