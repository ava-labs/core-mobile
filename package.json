{
  "name": "mobile-monorepo",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "all:setup": "yarn workspaces foreach -p run setup",
    "all:setup:ci": "yarn workspaces foreach -p run setup:ci",
    "all:lint": "yarn lint && yarn workspaces foreach -p run lint",
    "all:lint:ci": "yarn lint:ci && yarn workspaces foreach -p run lint:ci",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:ci": "yarn run lint --quiet --format json --output-file eslint_report.json"
  },
  "devDependencies": {
    "@avalabs/eslint-config-mobile": "workspace:*",
    "@rushstack/eslint-patch": "1.5.1",
    "eslint": "8.50.0",
    "husky": "8.0.1",
    "typescript": "4.7.4"
  },
  "resolutions": {
    "minimist": "1.2.6",
    "simple-plist": "1.3.1",
    "shell-quote": "1.7.3",
    "plist": "3.0.5",
    "ansi-regex": "4.1.1",
    "ansi-styles": "3.2.1",
    "node-fetch": "2.6.7",
    "@types/react": "17.0.44",
    "@types/react-dom": "18.0.6",
    "rxjs": "7.5.6",
    "eslint": "8.50.0",
    "eslint-config-prettier": "8.10.0"
  },
  "engines": {
    "node": ">=18.14.1",
    "yarn": ">=1.22.10"
  },
  "volta": {
    "node": "18.14.1",
    "yarn": "1.22.10"
  }
}
